{
  "name": "n8n-nodes-infomaniak",
  "version": "1.5.0",
  "description": "n8n node for Infomaniak API integration",
  "keywords": [
    "n8n-community-node-package",
    "infomaniak",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ascenziach/n8n-nodes-infomaniak",
  "author": {
    "name": "ascenziach",
    "email": "ascenziach@users.noreply.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ascenziach/n8n-nodes-infomaniak.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/InfomaniakApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/InfomaniakCoreResources/InfomaniakCoreResources.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
